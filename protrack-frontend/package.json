{
  "name": "protrack-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
    "lint:dev": "eslint . --ext ts,tsx --config .eslintrc.dev.cjs",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "setup-vscode": "./setup-vscode.sh",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "dev:debug": "vite --debug",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "demo": "bash run-demo.sh",
    "terminal-demo": "npx tsx src/terminal-demo.ts",
    "browser": "bash run-in-browser.sh"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@web3-react/core": "^8.2.3",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/metamask": "^8.2.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "ethers": "^6.13.4",
    "framer-motion": "^10.16.16",
    "i18next": "^23.7.8",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.303.0",
    "mapbox-gl": "^3.16.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "web3": "^4.16.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.6.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.12",
    "tsx": "^4.7.1"
  }
}
